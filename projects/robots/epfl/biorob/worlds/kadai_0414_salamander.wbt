  supervisor TRUE
}
DEF FLOOR Solid {
  children [
    DEF GND Group {
      children [
        Transform {
          # translation -4.42876 -0.25 0
          translation 0 -0.25 0          
          children [
            Shape {
              appearance DEF GROUND_APPEARANCE PBRAppearance {
                baseColorMap ImageTexture {
                  url [
                    "textures/texture_grid.png"
                  ]
                }
                roughness 1
                metalness 0
              }
              geometry Box {
                # size 3.14249 0.001 8
                # size 12 0.25 8
                size 8 0.25 6 
              }
            }
          ]
        }      
      ]
    }
  ]
  name "ground"
  contactMaterial "floor"
  boundingObject USE GND
  locked TRUE
}

DEF AQUARIUM Solid {
  translation 0 0.1 0
  children [
    DEF SIDES_GROUP Group {
      children [
        Transform {
          # translation 0 0 4
                    translation 0 0 3
          children [
            DEF GLASS_SHAPE1 Shape {
              appearance DEF GLASS_APPEARANCE PBRAppearance {
                # baseColor 1 0.6 0.6
                # baseColor 0 1 0
                baseColor 0 1 1
                roughness 1
                metalness 0
              }
              geometry Box {
                # size 12 0.7 0.02
                size 8 0.7 0.02
              }
            }
          ]
        }
        Transform {
          # translation 0 0 -4
          translation 0 0 -3          
          children [
            USE GLASS_SHAPE1
          ]
        }
        Transform {
          # translation -6 0 0
          translation 4 0 0
          rotation 0 1 0 1.5708
          children [
            DEF GLASS_SHAPE2 Shape {
              appearance USE GLASS_APPEARANCE
              geometry Box {
                # size 8 0.7 0.02
                size 6 0.7 0.02
              }
            }
          ]
        }
        Transform {
          # translation 6 0 0
          translation -4 0 0
          rotation 0 1 0 1.5708
          children [
            USE GLASS_SHAPE2
          ]
        }
      ]
    }
  ]
  name "aquarium"
  boundingObject USE SIDES_GROUP
  locked TRUE
}
DEF WATER Fluid {
  translation 0 -0.125 0
  children [
    DEF WATER_BOX Shape {
      appearance PBRAppearance {
        baseColor 0.7254901960784313 0.8705882352941177 1
        transparency 0.7
        roughness 0.2
        metalness 0
      }
      geometry Box {
        # size 12 0.25 8
        #contoroler のwater_levelなどとの兼ね合い
        size 8 0.25 6
        
      }
    }
  ]
  boundingObject USE WATER_BOX
  locked TRUE
  # name "swimming pool"  
}
